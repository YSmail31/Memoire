\begin{center}
\begin{algorithm}[H]
\KwData{Ensemble de processeur $\proc{}$, Temps Minimum d'execution $\wcet{SleepMin}$}
\KwResult{Ensemble de Tache d'endormissement $\taskset_{Sleep}$}
\Begin{
	\foreach{$ \proc{i} \in \proc{}$}
	{
		$\tache{sleep}^i$ = tache_endormissement_monoprocesseur($\wcet{SleepMin}$) \; 
		\if{$\wcet{sleep}^i \geq \wcet{SleepMin}$}
		$\taskset_{Sleep}^i =\taskset_{Sleep}^i \cup \tache{sleep}^i$
		}
	}
 \caption{Insertion Globale de Taches Endormissement Dans Une architecture Multiprocesseur}
\end{algorithm}
\end{center}